programa {
  funcao inicio() {
    
    real x, y, z, media

    escreva("Digite o primeiro número para descobrir a média: ")
      leia(x)

    escreva("Digite o segundo número: ")
      leia(y)

    escreva("Digite o segundo número ")
      leia(z)

    media = (x + y + z) / 3

    escreva("A média dos três números é ", media)

  }
}
